<packages>

  <package>
    <name>Terrain</name>
    <classes>
      <class>
        <stereotype>enumeration</stereotype><!--Placed above the class name with between << >>-->
        <name>Index3</name>
        <attributes>
          <attribute>
            <name>i</name><!--must have a name-->
            <visibility>+</visibility><!--+:public,#:protected,-:private,~:package-->
            <type>integer</type><!--any string, if another class will create an association-->
            <value>0</value><!--any string-->
          </attribute>
          <attribute>
            <name>j</name>
            <visibility>-</visibility>
            <type>integer</type>
            <value>0</value>
          </attribute>
          <attribute>
            <name>k</name>
            <visibility>#</visibility>
            <type>integer</type>
            <value>0</value>
          </attribute>
        </attributes>
        <methods>
          <method>
            <name>test</name>
            <visibility>+</visibility>
            <parameters>
              <parameter>
                <name>myFloat</name>
                <direction>in</direction>
                <type>float</type>
              </parameter>
            </parameters>
            <return-type>integer</return-type>
          </method>
        </methods>
        <dependencies><!--list explicit dependencies, overrides any implicit 
          dependencies discovered by inspecting types used in the class-->
          <dependency>
            <target></target><!--name of the class the dependency is on-->
            <type>dependency</type><!-- type can be dependency
              , association, aggregation, composition, generalization, 
                realization -->
            <tail-label></tail-label>
            <head-label></head-label>
          </dependency>
        </dependencies>
      </class>
    </classes>
  </package>
</packages>
